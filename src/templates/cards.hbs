{{#each this}}
<div class="photo-card">
  <a class="gallery__link" href="{{largeImageURL}}">
  <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" height="200"/>
  </a>
  <div class="info">
    <p class="info-item">
      <b>Likes</b><br>{{likes}}
    </p>
    <p class="info-item">
      <b>Views</b><br>{{views}}
    </p>
    <p class="info-item">
      <b>Comments</b><br>{{comments}}
    </p>
    <p class="info-item">
      <b>Downloads</b><br>{{downloads}}
    </p>
  </div>
</div>
{{/each}}